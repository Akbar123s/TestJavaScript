-- Fungsi untuk proses BillboardGui
local function processBillboardIfNeeded(obj)
	if obj:IsA("BasePart") and obj.Name == "Main" then
		local parent = obj.Parent
		while parent do
			if parent:IsA("Folder") and parent.Name == "Purchases" then
				for _, child in ipairs(obj:GetChildren()) do
					if child:IsA("BillboardGui") then
						child.Size = UDim2.new(0, 180, 0, 150)
						child.MaxDistance = 80
						child.StudsOffset = Vector3.new(0, 5, 0)
						print("âœ… BillboardGui diubah:", obj:GetFullName())
					end
				end
				break
			end
			parent = parent.Parent
		end
	end
end

-- Proses awal semua objek
for _, obj in ipairs(workspace:GetDescendants()) do
	processBillboardIfNeeded(obj)
end

-- Pantau objek baru yang masuk ke workspace
workspace.DescendantAdded:Connect(function(obj)
	processBillboardIfNeeded(obj)
end)











-- Fungsi untuk membuat ProximityPrompt UnlockBase tidak terlihat
local function makeUnlockBaseInvisible(obj)
	if obj:IsA("ProximityPrompt") and obj.Name == "UnlockBase" then
		local parent = obj.Parent
		while parent do
			if parent:IsA("Folder") and parent.Name == "Unlock" and parent:IsDescendantOf(workspace) then
				obj.Enabled = false -- Nonaktifkan prompt
				obj.RequiresLineOfSight = false -- Tidak perlu pandangan langsung
				obj.HoldDuration = 0 -- Opsional: biar tidak delay
				obj.MaxActivationDistance = 0 -- Ini membuatnya tidak bisa diaktifkan
				print("ðŸ«¥ Prompt disembunyikan:", obj:GetFullName())
				break
			end
			parent = parent.Parent
		end
	end
end

-- Cek semua di awal
for _, obj in ipairs(workspace:GetDescendants()) do
	makeUnlockBaseInvisible(obj)
end

-- Pantau yang baru muncul
workspace.DescendantAdded:Connect(function(obj)
	makeUnlockBaseInvisible(obj)
end)










