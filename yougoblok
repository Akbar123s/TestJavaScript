				1.08971683e-07, 1, 6.91502233e-09,
				-0.994276404, 1.07609189e-07, 0.106838159
			)

			humanoidRootPart.CFrame = cf
			humanoidRootPart.Anchored = true
			print("âœ… Teleport ke Shop dan Anchor aktif 2 detik.")

			task.delay(2, function()
				humanoidRootPart.Anchored = false
				print("ðŸ”“ Anchor dilepas.")
			end)
		end
	end
})


local VirtualUser = game:GetService("VirtualUser")
local Players = game:GetService("Players")
local player = Players.LocalPlayer

local antiAfkConnection = nil

AddToggle(miscTab, {
	Name = "Anti Afk",
	Default = false,
	Callback = function(v)
		print("Anti Afk:", v)

		if v then
			-- Aktifkan Anti AFK
			if antiAfkConnection then antiAfkConnection:Disconnect() end
			antiAfkConnection = player.Idled:Connect(function()
				VirtualUser:Button2Down(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
				wait(1)
				VirtualUser:Button2Up(Vector2.new(0, 0), workspace.CurrentCamera.CFrame)
				print("ðŸ›¡ï¸ Anti-AFK triggered.")
			end)
			print("âœ… Anti-AFK Aktif.")
		else
			-- Nonaktifkan Anti AFK
			if antiAfkConnection then
				antiAfkConnection:Disconnect()
				antiAfkConnection = nil
				print("â›” Anti-AFK Dimatikan.")
			end
		end
	end
})








local player = game:GetService("Players").LocalPlayer
local camera = workspace.CurrentCamera

local function removeLaggyStuff()
	for _, obj in pairs(workspace:GetDescendants()) do
		if obj:IsA("ParticleEmitter")
		or obj:IsA("Trail")
		or obj:IsA("Smoke")
		or obj:IsA("Fire")
		or obj:IsA("Explosion")
		or obj:IsA("Sound")
		or obj:IsA("Beam")
		or obj:IsA("Texture")
		or obj:IsA("Decal") then
			pcall(function() obj:Destroy() end)
		end
	end
end

local function optimizeLighting()
	pcall(function()
		local lighting = game:GetService("Lighting")
		lighting.FogEnd = 1e10
		lighting.FogStart = 1e10
		lighting.Brightness = 0
		lighting.GlobalShadows = false
	end)
end

local function clearCamera()
	camera:ClearAllChildren()
end

-- Toggle GUI
AddToggle(miscTab, {
	Name = "Anti lag",
	Default = false,
	Callback = function(v)
		print("Anti Lag:", v)
		if v then
			removeLaggyStuff()
			optimizeLighting()
			clearCamera()
			print("âœ… Anti Lag Aktif - Efek berat dibersihkan.")
		else
			print("â›” Anti Lag Nonaktif - Tidak mengembalikan efek yang dihapus.")
		end
	end
})





